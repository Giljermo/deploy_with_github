[Unit]
Description=uwsgi daemon
After=network.target
RuntimeDirectory=uwsgi

[Service]
User=mas
Group=www-data
WorkingDirectory=/home/mas/django_sites/deploy_with_github
ExecStart=/home/mas/django_sites/env/bin/uwsgi --ini /home/mas/django_sites/deploy_with_github/configs/dwg_uwsgi.ini

[Install]
WantedBy=multi-user.target
